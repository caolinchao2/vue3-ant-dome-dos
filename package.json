{
  "name": "galaxy-venus-fara",
  "version": "1.0.0-SNAPSHOT",
  "scripts": {
    "dev:mock": "vite --mode mock",
    "dev:mockSelf": "vite --mode mockSelf",
    "dev": "vite --mode development",
    "dev:debug": "vite --mode debug",
    "build:test": "vite build --mode test",
    "build:prod": "vite build",
    "build": "vite build --mode staging",
    "preview": "vite preview",
    "prepare": "husky install",
    "check": "vue-tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "subtree": "git remote add subtree https://github.com/wingjie/vue3-ant-dome-dos.git",
    "add": "git subtree add --prefix=dist subtree master",
    "pull": "git subtree pull --prefix=dist subtree master",
    "push": "git subtree push --prefix=dist subtree master"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@antv/x6": "^1.30.1",
    "@antv/x6-vue-shape": "^1.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.26.3",
    "@iconify/vue": "^4.1.2",
    "@micro-zoe/micro-app": "^0.8.5",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vueuse/core": "^10.7.0",
    "ant-design-vue": "^3.2.3",
    "axios": "^0.24.0",
    "dayjs": "^1.11.11",
    "galaxy-common-rely": "file:src/common",
    "highlight": "^0.2.4",
    "highlight.js": "^11.9.0",
    "insert-css": "^2.0.0",
    "js-beautify": "^1.14.3",
    "jsencrypt": "^3.3.2",
    "json-schema-faker": "^0.5.4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "marked": "^12.0.2",
    "moment": "^2.29.3",
    "qs": "^6.12.1",
    "sql-formatter": "^6.1.1",
    "vite-plugin-iconify-preset": "^1.0.2",
    "vite-plugin-qiankun": "^1.0.15",
    "vue": "^3.2.25",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0",
    "xlsx": "^0.18.5",
    "xml-formatter": "^2.6.1"
  },
  "devDependencies": {
    "@types/insert-css": "^2.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.47.0",
    "typescript": "^4.4.4",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.19.2",
    "vite": "^2.7.2",
