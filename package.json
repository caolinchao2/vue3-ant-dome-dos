    "*.{vue,js,ts}": "eslint --fix"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design/icons-vue",
        "@antv/x6",
        "@micro-zoe/micro-app",
        "ant-design-vue",
        "ant-design-vue/es/locale/zh_CN",
        "axios",
        "dayjs",
        "js-beautify",
        "lodash",
        "lodash-es",
        "moment",
        "sql-formatter",
        "vue",
        "vue-router",
        "vuex",
        "vuex-persistedstate",
        "xlsx",
        "xml-formatter"
      ]
    }
  }
}
